    <script>

        //Get title of recent posts
        var src = 'https://gifloop.tumblr.com/' + "rss";

        $.get(src, function (data) {
            $(data).find("item").first().each(function () { // or "item" or whatever suits your feed
                var el = $(this);

                var title = el.find("title").text().substr(0,50);
                var date = el.find("pubDate").text().substr(5,11);
                var author = el.find("creator").text();
                var summary = el.find("description").text().substr(0,75);
                var link = el.find("link").text();

                $("#recent").click(function(){
                    $("#sb-site").addClass( "slideUp" );
                      setTimeout(function(){
                          window.location= link;
                      },100);
                     return false;
                });

                document.getElementById("recentTitle").innerHTML=title + "...";
                document.getElementById("recentDate").innerHTML="on " + date;
                document.getElementById("recentAuthor").innerHTML=author;
                document.getElementById("recentSummary").innerHTML=summary + "...";


            });
        });
    </script>
    




